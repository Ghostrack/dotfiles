{
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "@/*": ["resources/assets/js/components/*"],
      "~/*": ["resources/js/*"],
      "@Components/*": ["resources/js/components/*"],
      "@Model/*": ["resources/js/Models/*"],
      "@Modal/*": ["resources/js/Modals/*"],
      "@Helper/*": ["resources/js/Helpers/*"],
      "@Modules/*": ["Modules/*"],
      "@Admin/*": ["Modules/Admin/Resources/assets/js/components/*"],
      "@Dashboard/*": ["Modules/Dashboard/Resources/assets/js/components/*"],
      "@FM/*": ["Modules/FM/Resources/assets/js/components/*"],
      "@Hr/*": ["Modules/Hr/Resources/assets/js/components/*"],
      "@Marketing/*": ["Modules/Marketing/Resources/assets/js/components/*"],
      "@Migration/*": ["Modules/Migration/Resources/assets/js/components/*"],
      "@Reports/*": ["Modules/Reports/Resources/assets/js/components/*"],
      "@Sales/*": ["Modules/Sales/Resources/assets/js/components/*"],
      "@ServiceDelivery/*": ["Modules/ServiceDelivery/Resources/assets/js/components/*"],
      "@Settings/*": ["Modules/Settings/Resources/assets/js/components/*"],
      "@Support/*": ["Modules/Support/Resources/assets/js/components/*"],
      "@Survey/*": ["Modules/Survey/Resources/assets/js/components/*"]
    }
  },
  "include": [
    "resources/js/**/*.vue",
    "resources/js/**/*.js",
    "resources/assets/js/**/*.vue",
    "resources/assets/js/**/*.js",
    "Modules/**/*.vue",
    "Modules/**/*.js"
  ]
}
